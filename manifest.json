{
  "name": "YouComment",
  "description": "Comment on Youtubeâ„¢ videos where comments are turned off",
  "version": "1.0",
  "manifest_version": 2,
  "key": "",
  "browser_action": {
    "default_popup": "Index.html",
    "default_icon": {
      "16": "src/YouComment.png",
      "32": "src/YouComment.png",
      "48": "src/YouComment.png",
      "128": "src/YouComment.png"
    }
  },
  "icons": {
    "16": "src/YouComment.png",
    "32": "src/YouComment.png",
    "48": "src/YouComment.png",
    "128": "src/YouComment.png"
  },
  "background": {
    "scripts": ["./background.js", "jsrsasign-all-min.js"]
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-beta/js/materialize.min.js; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*", "https://www.youtube.com/"],
      "js": ["jquery-3.5.1.min.js", "toChange.js"]
    }
  ],
  "permissions": [
    "identity",
    "identity.email",
    "activeTab",
    "declarativeContent",
    "storage",
    "https://www.youtube.com/",
    "webNavigation",
    "tabs"
  ]
}
